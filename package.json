    "@tsconfig/node12": "^1.0.7",
    "@types/express": "^4.17.8",
    "@types/mongoose": "^5.7.37",
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-eslint": "^10.0.0",
    "concurrently": "^4.0.0",
    "eslint": "^7.19.0",
